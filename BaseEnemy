public class BaseEnemy extends Enemy
{
   private final double speed;
   private int health;    
   
   public BaseEnemy()
   {
       speed = 0;
       health = 150;
   }
   
   public double getSpeed()
   {
       return speed;
   }
   
   public int getHealth()
   {
       return health;
   }
   
   public void upgrade()
   {
       health += 35;
   }
